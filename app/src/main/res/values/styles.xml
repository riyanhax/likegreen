<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!--全屏-->
    <style name="AppThemeFullscreen" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AppThemeProgress">
        <item name="colorAccent">#dfdfdf</item> <!-- Whatever color you want-->
    </style>

    <style name="admsginputtext">
        <item name="android:textColor">#404040</item>
        <item name="android:textSize">13sp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="paddinglift">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_width">@dimen/dp_9</item>
        <item name="android:layout_height">@dimen/dp_16</item>
    </style>

    <style name="paddingright">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_width">@dimen/dp_16</item>
        <item name="android:layout_height">@dimen/dp_16</item>
    </style>

    <style name="popiv">
        <!-- Customize your theme here. -->
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_height">16dp</item>
    </style>

    <style name="poptv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#515151</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>
    <!-- Dialog -->
    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="head">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="systemtext">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="systeimg">
        <item name="android:layout_width">12dp</item>
        <item name="android:layout_height">12dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="Transparent">
        <item name="android:windowBackground">
            @color/transparent_background
        </item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>

    </style>


